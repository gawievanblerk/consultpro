# ConsultPro Backend Environment Variables

# Server
NODE_ENV=development
PORT=4020

# Database
DATABASE_URL=postgres://consultpro:consultpro_secret@localhost:5432/consultpro
# Or use individual variables:
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=consultpro
# DB_USER=consultpro
# DB_PASSWORD=consultpro_secret

# Authentication
JWT_SECRET=your-super-secret-jwt-key-change-in-production
DEMO_MODE=true

# Nigeria Tax Rates (defaults if not set)
# NIGERIA_VAT_RATE=7.5
# NIGERIA_WHT_SERVICES=5
# NIGERIA_WHT_PROFESSIONAL=10
